  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:3001",
      "webRoot": "${workspaceFolder}/identity.web"
    },
    {
    "type": "node",
    "request": "launch",
    "name": "Launch via NPM",
    "runtimeExecutable": "npm",
    "cwd": "${workspaceFolder}/identity.api",
    "runtimeArgs": [
      "run",
      "debug"
    ],
    "port": 9222
  }]
}
